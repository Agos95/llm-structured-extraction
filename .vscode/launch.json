    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "app.py",
                "--server.runOnSave",
                "true"
            ],
            "envFile": ""
        },
        {
            "name": "Gradio",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/gradio",
            "args": [
                "app_gradio.py"
            ],
            "envFile": ""
        },
    ]
}